                cannvasLineGoDraw(targetX,targetY){
                    this.node = document.getElementById('inc-canvas');
                    this.cxt = this.node.getContext("2d");
                    this.cxt.clearRect(0, 0, 800, 600);
                    this.cxt.beginPath();
                    this.cxt.moveTo(160, 160);
                    this.cxt.lineTo(160 + targetX, 160 + targetY);
                    this.cxt.lineWidth = 1;
                    this.cxt.strokeStyle = "rgb(44, 155, 254)";
