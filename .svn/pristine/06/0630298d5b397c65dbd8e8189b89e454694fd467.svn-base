                    if(this.timer) 
                        clearInterval(this.timer);
                    if(this.cxt)
                        this.cxt.clearRect(0, 0, 800, 600);
                    targetX = targetX ? targetX - 160 : 0;
                    targetY = targetY ? targetY - 160 : 0;
                    let theTime = new Date();
                    this.timer = setInterval(res=>{
                        let timeDif = this.easing((new Date() - theTime) / 2000);
                        if(timeDif >= 1){
                            timeDif = 1;
                            clearInterval(this.timer);
                        }
                        this.cannvasLineGoDraw(targetX * timeDif, targetY * timeDif);
                    },1000/60);
